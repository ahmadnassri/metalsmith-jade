  "devDependencies": {
    "codeclimate-test-reporter": "0.1.0",
    "echint": "^1.3.0",
    "istanbul": "^0.3.17",
    "metalsmith": "^1.7.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.1",
    "should": "^8.2.1",
    "standard": "^4.5.2"
  },
  "dependencies": {
    "debug-log": "^1.0.0",
    "jade": "^1.11.0",
    "xtend": "^4.0.0"
  }
}
